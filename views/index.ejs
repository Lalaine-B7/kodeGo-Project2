<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@4.1.3/dist/css/bootstrap.min.css" integrity="sha384-MCw98/SFnGE8fJT3GXwEOngsV7Zt27NXFoaoApmYm81iuXoPkFOJwJ8ERdknLPMO" crossorigin="anonymous">
    <title>Mini Project2</title>
</head>
<body class="container">
    <h1><%=header%></h1>
    <table class=" table table-boardered table-striped table-hover">
        <thead class="table-dark">
            <tr>
            <th>ID</th>
            <th>CAR_NAME</th>
            <th>BRAND</th>
            <th>BODY_TYPE</th>
            <th>YEAR</th>
            <th>TRANSMISSION</th>
            <th>TYPE</th>
            <th>ENGINE</th>
            <th>SEAT</th>
            <th>COLOR</th>
            <th>CONDITION</th>
            </tr>
        </thead>
        <tbody>
            <%records.forEach(function(cars){ %>
                <tr>
                    <td>
                        <%= cars.ID%>
                    </td>
                    <td>
                        <%= cars.CAR_NAME%>
                    </td>
                    <td>
                        <%= cars.BRAND%>
                    </td>
                    <td>
                        <%= cars.BODY_TYPE%>
                    </td>
                    <td>
                        <%= cars.YEAR%>
                    </td>
                    <td>
                        <%= cars.TRANSMISSION%>
                    </td>
                    <td>
                        <%= cars.TYPE%>
                    </td>
                    <td>
                        <%= cars.ENGINE%>
                    </td>
                    <td>
                        <%= cars.SEAT%>
                    </td>
                    <td>
                        <%= cars.COLOR%>
                    </td>
                    <td>
                        <%= cars.CONDITION%>
                    </td>
                </tr>
            <%}) %>
        </tbody>
    </table>
    <a href="/add" class="btn btn-dark"> âž• add </a>
       
</body>
</html>